    "nodemon": "^2.0.15",
    "nyc": "^15.1.0",
    "ts-node": "^10.5.0",
    "tsc-alias": "^1.5.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "helmet": "^5.0.2",
    "log4js": "^6.4.1",
    "mongoose": "^6.2.1",
    "multer": "^1.4.4",
    "nanoid": "^3.3.1"
