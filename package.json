  },
  "homepage": "https://github.com/mukeshgurpude/resourcely-backend#readme",
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.15",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "eslint": "^8.8.0",
