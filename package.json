  "description": "",
  "bin": {
    "cleanup": "./bin/cleanup.mjs"
  },
  "scripts": {
    "build": "tsc && tsc-alias",
    "dev": "nodemon -r tsconfig-paths/register src/server.ts",
    "lint": "eslint . --ext .ts",
